FLASK_APP=app
FLASK_ENV=development
FLASK_DEBUG=1

# Local Database Settings
SQLALCHEMY_DATABASE_URI=postgresql://postgres:admin@localhost/cms_local
SECRET_KEY=your-local-secret-key

# Mail settings (use your local SMTP settings if needed)
MAIL_SERVER=localhost
MAIL_PORT=25
MAIL_USE_TLS=false
MAIL_USERNAME=
MAIL_PASSWORD=

# Other settings
UPLOAD_FOLDER=uploads
ALLOWED_EXTENSIONS=png,jpg,jpeg,gif,pdf,doc,docx
MAX_CONTENT_LENGTH=16777216
